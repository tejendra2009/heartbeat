        <domain includeSubdomains="true">192.168.1.9</domain>
        <base-config cleartextTrafficPermitted="false">
            <trust-anchors>
                <certificates src="system" />
            </trust-anchors>
        </base-config>
    </domain-config>

</network-security-config>